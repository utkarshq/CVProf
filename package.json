{
  "name": "cv-generator",
  "version": "1.0.0",
  "description": "Professional CV/Resume generator supporting PDF (LaTeX) and Web (JSON Resume) formats with multi-language support",
  "author": "Utkarsh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/utkarshq/cv"
  },
  "scripts": {
    "build": "python3 scripts/build.py",
    "build:web": "python3 scripts/build.py --web",
    "build:pdf": "python3 scripts/build.py --one-page --two-page"
  },
  "dependencies": {
    "handlebars": "^4.7.8",
    "jsonresume-theme-stackoverflow": "^2.1.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {},
  "keywords": [
    "cv",
    "resume",
    "latex",
    "json-resume",
    "multilingual",
    "pdf-generator"
  ],
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  }
}